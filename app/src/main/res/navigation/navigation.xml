<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="org.ieselcaminas.luisdaniel.proyectosinnombre.ui.main.MainFrag"
        android:label="main_fragment"
        tools:layout="@layout/main_frag_layout" >
        <action
            android:id="@+id/action_mainFrag_to_loginFrag"
            app:destination="@id/loginFrag"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/loginFrag"
        android:name="org.ieselcaminas.luisdaniel.proyectosinnombre.login.LoginFrag"
        android:label="login_frag_layout"
        tools:layout="@layout/login_frag_layout" />
</navigation>